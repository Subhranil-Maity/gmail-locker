{
  "manifest_version": 3,
  "name": "Gmail Locker",
  "version": "1.0",
  "permissions": ["storage", "tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}

